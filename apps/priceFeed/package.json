{
  "name": "@trading-bot/price-feed",
  "version": "1.0.0",
  "engines ": {
    "node": "18.12.0"
  },
  "description": "Trading bot which will use AI, vector database and othe modern technology to get the best profit from their trading strategy",
  "main": "src/app.ts",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "dev": "NODE_ENV=development dotenv_config_path=.env.development nodemon",
    "start": "NODE_ENV=production node ./dist/app.js",
    "build": "rollup -c --bundleConfigAsCjs",
    "watch": "rollup -c -w --bundleConfigAsCjs",
    "prettier": "prettier --write \"src\" --ignore-path .gitignore",
    "lint": "npx eslint ."
  },
  "keywords": [
    "ai",
    "cryptocurrency",
    "crypto",
    "vector-database",
    "openai",
    "tradinf",
    "trader",
    "trading-bot"
  ],
  "author": "Antoni Kois",
  "license": "ISC",
  "dependencies": {
    "@binance/connector": "^2.0.1",
    "@trading-bot/tsconfig": "^0.0.0",
    "bybit-api": "^3.5.5",
    "dotenv": "^16.0.3",
    "eslint": "^8.38.0",
    "eslint-config-trading-bot-node": "^0.0.0",
    "prettier": "^2.8.7",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  }
}
